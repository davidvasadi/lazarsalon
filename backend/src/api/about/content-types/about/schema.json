      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "AboutService": {
      "type": "component",
      "repeatable": true,
      "component": "about-service-item.about-service-item"
    },
    "Introdution": {
      "type": "text"
    },
    "openingHours": {
      "type": "component",
      "repeatable": true,
      "component": "opening-hours.opening-hours"
    }
  }
}
