{
  "kind": "collectionType",
  "collectionName": "service_cards",
  "info": {
    "singularName": "service-card",
    "pluralName": "service-cards",
    "displayName": "Service Card",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ServiceCard": {
      "type": "component",
      "repeatable": true,
      "component": "services.service-item"
    },
    "ctaType": {
      "type": "enumeration",
      "enum": [
        "booking",
        "contact"
      ]
    },
    "phoneNumber": {
      "type": "string"
    }
  }
}
